{
  "name": "my-next-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -r \"npx next\" \"npm run watch:prisma\" \"npm run watch:relay\"",
    "relay": "relay-compiler",
    "build": "next build",
    "start": "next start",
    "schema": "ts-node -O '{\"module\":\"CommonJS\"}' --transpile-only ./graphql/schema.ts",
    "watch:prisma": "onchange prisma/**/*.prisma -- prisma generate",
    "watch:relay": "relay-compiler --watch",
    "type-check": "tsc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "test": "jest"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.1.4",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@prisma/client": "^2.14.0",
    "@zeit/next-source-maps": "^0.0.3",
    "apollo-server-micro": "3.0.0-alpha.3",
    "comlink": "^4.3.0",
    "framer-motion": "^3.2.1",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.5",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.28.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-relay": "^10.1.2",
    "relay-hooks": "^4.0.0-rc.4",
    "stylis": "^3.5.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.14.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-relay": "^7.0.17",
    "@types/react-test-renderer": "^17.0.0",
    "@types/relay-runtime": "^10.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-plugin-relay": "^10.1.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-relay": "^1.8.1",
    "graphql": "^15.4.0",
    "jest": "^26.6.3",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "relay-compiler": "^10.1.2",
    "relay-compiler-language-typescript": "^13.0.2",
    "relay-config": "^10.1.2",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vscode-apollo-relay": "^1.5.1",
    "worker-plugin": "^5.0.0"
  },
  "license": "MIT"
}
